cloudflare works  脚本

addEventListener(
"fetch",event => {
let url=new URL(event.request.url);
url.hostname="binky2020.us-south.cf.appdomain.cloud";
url.pathname="/109c563a";
let request=new Request(url,event.request);
event. respondWith(
fetch(request)
)
}
)
